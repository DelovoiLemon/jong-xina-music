{
  "name": "jong-xina-discordmusicbot",
  "version": "3.0.0",
  "description": "A discord bot that plays music from youtube and local files and can also apply ffmpeg audio effects. ",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./**/*.ts",
    "fix": "eslint ./**/*.ts --fix",
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "pretest": "npm run build",
    "test": "mocha --require source-map-support/register --file ./build/test_utils/global_hooks.test.js ./build/**/*test.js",
    "posttest": "npm run lint",
    "coverage": "nyc npm test",
    "prestart": "npm run build",
    "start": "node -r source-map-support/register build/xina"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DelovoiLemon/jong-xina-music.git"
  },
  "author": "DelovoiLemon",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DelovoiLemon/jong-xina-music/issues"
  },
  "homepage": "https://github.com/DelovoiLemon/jong-xina-music#readme",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.17",
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/fs-extra": "^11.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.10",
    "@types/node-ipc": "^9.2.0",
    "@types/sinon": "^10.0.13",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "chai": "^4.3.7",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.35.0",
    "gts": "^3.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.0.2",
    "source-map-support": "^0.5.21",
    "typed-emitter": "^2.1.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@discordjs/voice": "^0.15.0",
    "@mapbox/node-pre-gyp": "^1.0.10",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "express": "^4.18.2",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "fs-extra": "^11.1.0",
    "fuse.js": "^6.6.2",
    "mongodb": "^5.1.0",
    "music-metadata": "^8.1.3",
    "node-ipc": "^10.1.0",
    "path": "^0.12.7",
    "sqlite3": "^5.1.6",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1",
    "ytdl-core": "^4.11.2",
    "ytpl": "^2.3.0",
    "ytsr": "^3.8.0"
  }
}
